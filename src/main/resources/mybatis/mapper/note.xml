<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.newlecture.web.dao.NoteDao">
	<select id="getAllNote" parameterType="com.newlecture.web.entity.NoteEntity">
		select * from note_tb
	</select>

	<select id="getCommentByBoard" parameterType="com.newlecture.web.entity.NoteEntity">
		select * from note_tb
		where send_id = #{send_id}
	</select>
</mapper>